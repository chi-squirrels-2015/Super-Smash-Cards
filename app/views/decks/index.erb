<div class="container">
<input type="hidden" name="user_id" value="<%= session[:user_id] %>">
  <h1>Choose Your Destiny</h1>

  <% @decks.each do |deck| %>
    <p><a href="/decks/<%= deck.title %>/cards"><%= deck.title %></a></p>
  <% end %>

  <button type="submit"><a href="/users">Log Out</a></button>

</div>
